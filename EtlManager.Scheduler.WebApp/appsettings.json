{
  "ConnectionStrings": {
    "EtlManagerContext": "Server=localhost;Database=EtlManager;Integrated Security=true;MultipleActiveResultSets=true"
  },
  "Executor": {
    "Type": "ConsoleApp", // [ ConsoleApp | WebApp ]
    "ConsoleApp": {
      "EtlManagerExecutorPath": "C:\\EtlManagerExecutor\\EtlManagerExecutor.exe"
    },
    "WebApp": {
      "Url": "http://localhost:5139"
    }

  },
  "Kestrel": {
    "Endpoints": {
      "Http": { "Url": "http://localhost:5432" },
      "Https": { "Url": "https://localhost:5433" }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\EtlManagerScheduler\\log\\scheduler.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss.fff zzz} [{Level:w3}] {Message:lj}{NewLine}{Exception}",
          "shared": true
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
