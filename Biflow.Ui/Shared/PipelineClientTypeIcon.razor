
@switch (ClientType)
{
    case PipelineClientType.DataFactory:
        <SvgIcon Icon="LucideIcon.Factory" />
        break;
    case PipelineClientType.Synapse:
        <SvgIcon Icon="FeatherIcon.Cpu" />
        break;
    default:
        throw new ArgumentOutOfRangeException();
}

@code {
    [Parameter] public PipelineClientType ClientType { get; set; }
}
