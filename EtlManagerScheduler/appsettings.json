{
  "EtlManagerConnectionString": "Data Source=localhost;Initial Catalog=EtlManager;Integrated Security=SSPI;",
  "EtlManagerExecutorPath": "C:\\EtlManagerExecutor\\EtlManagerExecutor.exe",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\EtlManagerScheduler\\log\\scheduler.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:HH:mm:ss.fff zzz} [{Level:w3}] {Message:lj}{NewLine}{Exception}",
          "shared": true
        }
      }
    ]
  }
}
